<div class="row" id="app">
  <div class="col-md-3 sidebar">
    <%= render :partial => 'namespace/sidebar'%>
  </div>
  <div class="col-md-9">
    <div class="container">
      <% @configsdifference.each do |key,val|%>
        <div class="row justify-content-center">
          <h2>Difference with Parent Version: <%= key %></h2>
          <div class="col-md-12">
              <table class="table table-hover text-center">
              <thead>
                <th class="text-center">Type of difference</th>
                <th class="text-center">Differences</th>
              </thead>
              <tbody>
                <tr>
                  <td>Deleted</td>
                  <td><%= val[0]%></td>
                </tr>
                <tr>
                  <td>Added</td>
                  <td><%= val[1]%></td>
                </tr>
                <tr>
                  <td>Updated</td>
                  <td><%= val[2]%></td>
                </tr>
              </tbody>
              </table>
          </div>
        </div>
        <br>
      <% end %>
    </div>
  </div>
</div>