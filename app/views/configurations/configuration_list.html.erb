<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <h3> Version : <%= @version %>
        <a href="/namespace/edit/<%=@servicename%>/<%= @namespace%>/<%= @version%>" 
        class="btn btn-success">
          Edit
        </a>
      </h3>
    </div>
  </div>
  <div class="row">
  <div class="col-md-10 col-md-offset-1">
    <table class="table table-hover text-center">
    <thead>
      <th class="text-center">Key</th>
      <th class="text-center">Value</th>
    </thead>
    <tbody>
      <% for i in 0..@configkeys.length %>
      <tr>
        <td><%= @configkeys[i]%></td>
        <td><%= @configvalues[i]%></td>
      </tr>
      <% end %>
    </tbody>
    </table>
  </div>
  </div>
</div>
