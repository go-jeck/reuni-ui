<div class="container">
<br>
  <div class="row justify-content-center">
    <div class="col-md-10 offset-md-1">
      <div class="row">
        <h4 class="col-md-5 offset-md-1"> Version : <%= @version %> </h4>
        <div class="col-md-4  text-right">
          <a href="/namespace/edit/<%=@servicename%>/<%= @namespace%>/<%= @version%>" 
          class="btn btn-success">
            Edit
          </a>
        </div>
      </div>
    </div>
  </div>
  <br>
  <div class="row justify-content-center">
  <div class="col-md-6">
    <table class="table table-hover text-center">
    <thead>
      <th class="text-center">Key</th>
      <th class="text-center">Value</th>
    </thead>
    <tbody>
      <% for i in 0..@configkeys.length %>
      <tr>
        <td><%= @configkeys[i]%></td>
        <td><%= @configvalues[i]%></td>
      </tr>
      <% end %>
    </tbody>
    </table>
  </div>
  </div>
</div>
